{
  "name": "reactstrap-date-picker",
  "version": "0.1.0",
  "description": "Reactstrap date picker",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "reactstrap": "^5.0.0-alpha.4"
  },
  "keywords": [
    "react",
    "react-component",
    "reactstrap",
    "bootstrap",
    "date picker",
    "calendar",
    "date",
    "picker",
    "reactstrap-date-picker"
  ],
  "scripts": {
    "start": "npm run build",
    "build": "webpack --config ./example/webpack.example.config.js -d && webpack-dev-server --inline --content-base ./example --config ./example/webpack.example.config.js --hot",
    "build:prod": "webpack -p",
    "lint": "eslint --ext .jsx src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./__test__/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "react-dom": "^16.2.0",
    "react-test-render": "^1.1.1",
    "react-transition-group": "^2.2.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "pre-push": []
}
